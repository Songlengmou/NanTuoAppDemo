            android:name=".activity.aroundlife.PoiSearchActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <!-- 鹰眼足迹总体 -->
        <activity
            android:name=".activity.footprint.TraceMainActivity"
            android:screenOrientation="portrait" />
        <!-- 轨迹实时追踪 -->
        <activity
            android:name=".activity.footprint.TracingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.footprint.TrackQueryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.footprint.TrackQueryOptionsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppBaseTheme" />
        <!-- 语音导航信息 -->
        <activity
            android:name=".activity.voice.NaviActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.voice.NaviGuideActivity"
            android:configChanges="orientation|screenSize|keyboard|keyboardHidden"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.footprint.TracingOptionsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppBaseTheme" />
        <!-- 功能选择 -->
        <activity android:name=".activity.ChooseMapFunctionActivity" />
        <!-- 离线地图下载 -->
        <activity android:name=".activity.offline.OfflineMainActivity" />
        <activity android:name=".activity.offline.OfflineMapActivity" />
        <activity android:name=".activity.offline.BaseMapOfflineActivity" />
        <!-- 登录页 -->
        <activity android:name=".activity.login.LoginActivity" />
        <!-- 注册页 -->
        <activity android:name=".activity.register.RegisterActivity" />
        <!-- 聊天整体界面控制 -->
        <activity android:name=".activity.social.SocialActivity" />
        <!-- 主页 -->
        <activity
            android:name=".activity.MainActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />
        <!-- 语种选择页 -->
        <activity android:name=".activity.LanguageSelectionActivity" />
        <!-- 添加好友 -->
        <activity android:name=".activity.social.AddFriendActivity" />
        <!-- 创建群组界面 -->
        <activity android:name=".activity.social.CreateGroupActivity" />
        <!-- 个人聊天详情页 -->
        <activity android:name=".activity.social.PrivateMessageActivity" />
        <!-- 个人聊天详情页的图片放大 目前还不完善 需修改 TODO -->
        <activity android:name=".activity.social.PictureBigActivity" />
        <!-- 个人聊天所发视频页 -->
        <activity android:name=".activity.social.VideoViewActivity" />
